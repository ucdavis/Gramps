      </Fields>
      <Query>
        <DataSourceName>GrampsDataSource</DataSourceName>
        <CommandText>SELECT     CallForProposals.Description, CallForProposals.ProposalMaximum, CallForProposals.CreatedDate, CallForProposals.Name, CallForProposals.id, 
                      Proposals.id AS ProposalId, Proposals.CallForProposalId, Proposals.IsSubmitted, Proposals.Sequence, CallForProposals.EndDate, 
                      CallForProposals.CallsSentDate
FROM         CallForProposals INNER JOIN
                      Proposals ON CallForProposals.id = Proposals.CallForProposalId
WHERE     (CallForProposals.id = @CallId) AND (Proposals.IsSubmitted = 1) AND (@ProposalId = '') OR
                      (CallForProposals.id = @CallId) AND (Proposals.IsSubmitted = 1) AND (Proposals.id = @ProposalId)
ORDER BY Proposals.Sequence</CommandText>
        <QueryParameters>
          <QueryParameter Name="@CallId">
            <Value>=Parameters!CallId.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ProposalId">
            <Value>=Parameters!ProposalId.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
